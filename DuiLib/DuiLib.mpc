project(DuiLibus):build_files,lukestatic,lukepch,DuilibOut{
    sharedname=DuiLibus
    dynamicflags=UILIB_EXPORTS
    includes+=../include

    Header_Files{
        *.h
        Control\*.h
        Core\*.h
        Layout\*.h
        Utils\*.h
    }

    Source_Files{
        *.cpp
        Control\*.cpp
        Core\*.cpp
        Layout\*.cpp
        Utils\*.cpp
        no_pch=1
        Utils\XUnzip.cpp
        Utils\*.c
    }
}
